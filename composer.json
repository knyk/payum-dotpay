{
    "name": "knyk/sylius-dotpay-plugin",
    "type": "sylius-plugin",
    "description": "Dotpay implementation for Sylius",
    "keywords": [
        "payum",
        "dotpay",
        "sylius",
        "sylius-plugin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Falgowski",
            "email": "kamfalg@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "sylius/sylius": "^1.7",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpspec/phpspec": "^6.1",
        "phpstan/phpstan": "^0.12.18"
    },
    "autoload": {
        "psr-4": {
            "Knyk\\SyliusDotpayPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/"
        }
    }
}
